{
  "variables": [
      {
          "name": "start_time",
          "type": "PX_STRING",
          "description": "The relative start time of the window. Current time is assumed to be now",
          "defaultValue": "-5m"
      },
      {
          "name": "service",
          "type": "PX_SERVICE",
          "description": "The name of the service to get stats for. Format: namespace/svc_name"
      },
      {
          "name": "endpoint",
          "type": "PX_STRING",
          "description": "The endpoint to look at stats for. Format: /path/to/endpoint/*/more/path "
      }
  ],
  "globalFuncs": [],
  "widgets": [
      {
          "name": "Sample of Endpoint Requests",
          "position": {
              "x": 0,
              "y": 0,
              "w": 12,
              "h": 3
          },
          "func": {
              "name": "endpoints",
              "args": [
                  {
                      "name": "start_time",
                      "variable": "start_time"
                  },
                  {
                      "name": "service",
                      "variable": "service"
                  },
                  {
                      "name": "endpoint",
                      "variable": "endpoint"
                  }
              ]
          },
          "displaySpec": {
              "@type": "types.px.dev/px.vispb.Table"
          }
      }
  ]
}
